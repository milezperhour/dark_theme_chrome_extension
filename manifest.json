{
    "name": "Dark Theme",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Darkens background color of webpages",
    "icons": {},
    "content_scripts": [
        {
            "js": ["js/jquery-3.3.1.min.js", "js/content.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": [
        "background",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
